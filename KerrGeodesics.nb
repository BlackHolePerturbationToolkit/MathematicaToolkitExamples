Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"KerrEqProgradePlot", "[", 
    RowBox[{"a_", ",", "p_", ",", "e_", ",", "\[Chi]max_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "orbit", ",", "rmax", ",", "t", ",", "r", ",", "\[Theta]", ",", 
       "\[Phi]", ",", "stable"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"stable", "=", 
       RowBox[{"KerrGeoStableOrbitQ", "[", 
        RowBox[{"a", ",", "p", ",", "e", ",", "0"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"stable", "\[Equal]", "True"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"orbit", "=", 
          RowBox[{"KerrGeoOrbit", "[", 
           RowBox[{"a", ",", "p", ",", "e", ",", "0"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}], 
          "=", 
          RowBox[{"orbit", "[", "\"\<Interpolation\>\"", "]"}]}], ";"}]}], 
       RowBox[{"(*", 
        RowBox[{
        "The", " ", "interpolated", " ", "version", " ", "is", " ", "faster", 
         " ", "to", " ", "evaluate"}], "*)"}], "\[IndentingNewLine]", "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"rmax", "=", 
       RowBox[{
        FractionBox["p", 
         RowBox[{"1", "-", "e"}]], "+", "1"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Show", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"stable", "\[Equal]", "True"}], ",", "\[IndentingNewLine]", 
          
          RowBox[{"ParametricPlot", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{"r", "[", "\[Chi]", "]"}], 
               RowBox[{"Cos", "[", 
                RowBox[{"\[Phi]", "[", "\[Chi]", "]"}], "]"}]}], ",", 
              RowBox[{
               RowBox[{"r", "[", "\[Chi]", "]"}], 
               RowBox[{"Sin", "[", 
                RowBox[{"\[Phi]", "[", "\[Chi]", "]"}], "]"}]}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"\[Chi]", ",", "0", ",", "\[Chi]max"}], "}"}], ",", 
            RowBox[{"ImageSize", "\[Rule]", "600"}], ",", 
            RowBox[{"PlotPoints", "\[Rule]", "100"}], ",", 
            RowBox[{"Frame", "\[Rule]", "True"}], ",", 
            RowBox[{"Axes", "\[Rule]", "False"}]}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"Graphics", "[", 
           RowBox[{"Text", "[", 
            RowBox[{
             RowBox[{"Style", "[", 
              RowBox[{"\"\<No stable orbit for those parameters\>\"", ",", 
               RowBox[{"FontSize", "\[Rule]", "20"}]}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"0", ",", 
               RowBox[{"rmax", "-", "1"}]}], "}"}]}], "]"}], "]"}]}], 
         "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Graphics", "[", 
         RowBox[{"Disk", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0"}], "}"}], ",", 
           RowBox[{"1", "+", 
            RowBox[{"Sqrt", "[", 
             RowBox[{"1", "-", 
              SuperscriptBox["a", "2"]}], "]"}]}]}], "]"}], "]"}], ",", 
        RowBox[{"PlotRange", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
