          RowBox[{"180", 
           FractionBox["\[Theta]", "\[Pi]"]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]", ",", 
          FractionBox["\[Pi]", "n"]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rms", "=", 
      RowBox[{"Table", "[", 
